terraform {
  required_providers {
      aws = {
        source  = "hashicorp/aws"
        version = "~> 4.16"
      }
    }
  required_version = ">= 0.14.9"
  backend "s3" {
    bucket = "<BUCKET_STATE_NAME>"
    key = "sqs-proxy-rabbitmq.tfstate"
    region = "<BUCKET_STATE_REGION>"
  }
}

provider "aws" {
  region = "<BUCKET_RESOURCES_REGION>"
}